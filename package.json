{
  "name": "ek-bff",
  "version": "0.1.0",
  "description": "BFF for EK",
  "author": "EK",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:copy": "cpx \"./src/assets/**/*\" \"./dist/assets\"",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "run-s prebuild build:copy start:debug",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "release": "run-s lint test:ci test:e2e-ci build",
    "test": "jest --watchAll --colors --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --bail --no-colors --ci --detectOpenHandles --no-cache",
    "test:e2e": "jest --config ./test/jest-e2e.json --watchAll --no-cache",
    "test:e2e-ci": "jest --config ./test/jest-e2e.json --bail --no-colors --ci --detectOpenHandles --no-cache"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/microservices": "^7.6.13",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/serve-static": "^2.1.4",
    "ajv": "^6.12.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-redis": "^5.1.0",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nestjs-redis": "^1.2.8",
    "nestjs-session": "^1.0.1",
    "q": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rsa-pem-from-mod-exp": "^0.8.4",
    "rxjs": "^6.6.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.8",
    "@nestjs/testing": "^7.6.13",
    "@types/connect-redis": "0.0.16",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/q": "^1.5.4",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "cpx": "^1.5.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.2",
    "jest": "26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rxjs-marbles": "^6.0.1",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  }
}
